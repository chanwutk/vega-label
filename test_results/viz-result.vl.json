{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "data": {
    "url": "https://gist.githubusercontent.com/chanwutk/c087f4caa1d8ccdbb5f64fe686841524/raw/d5c1832bb064f1f3b9c8e01a0ea307be4fc365b2/label-result-mark-2020-04-29.json"
  },
  "transform": [
    {
      "calculate": "datum.labeler === 'particle' ? 'Particle-Based': 'Bitmap-Based'",
      "as": "labeler"
    }
  ],
  "facet": {
    "column": {
      "field": "chart_width",
      "type": "nominal",
      "title": "Chart width and height"
    },
    "row": {
      "field": "type",
      "type": "nominal",
      "title": "Data Points Distribution"
    }
  },
  "spec": {
    "layer": [
      {
        "mark": {"type": "line", "point": true},
        "encoding": {
          "y": {
            "field": "runtime",
            "type": "quantitative",
            "aggregate": "median",
            "title": "Median of Running Time (ms)"
          },
          "color": {
            "field": "labeler",
            "type": "nominal",
            "title": "Labeling Algorithm"
          },
          "x": {
            "field": "num_point",
            "type": "quantitative",
            "title": "Number of Data Points"
          }
        }
      }
    ]
  }
}