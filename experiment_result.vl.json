{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "data": {
    "url": "https://gist.githubusercontent.com/chanwutk/84c325749cc37f5bb6866c5bcc04de00/raw/c39499eba996c794a8eab92850ea223d5dd876ae/label_time_4.json"
  },
  "facet": {
    "column": {
      "field": "width",
      "type": "nominal",
      "title": "Chart width (pixel)"
    }
  },
  "spec": {
    "width": 400,
    "height": 600,
    "layer": [
      {
        "mark": { "type": "line", "interpolate": "monotone", "point": true },
        "encoding": {
          "x": {
            "field": "input_size",
            "type": "quantitative",
            "title": "Input size"
          },
          "y": {
            "aggregate": "median",
            "field": "time",
            "type": "quantitative",
            "title": "Time (ms)"
          },
          "color": { "field": "method", "type": "nominal" },
          "detail": { "field": "input_shape", "type": "nominal" },
          "opacity": {
            "condition": {
              "test": "datum.input_shape==='clustered'",
              "value": 1
            },
            "value": 0.4
          }
        }
      }
    ]
  }
}
